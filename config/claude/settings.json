{
  "env": {
    "DISABLE_BUG_COMMAND": "1",
    "DISABLE_ERROR_REPORTING": "1",
    "DISABLE_NON_ESSENTIAL_MODEL_CALLS": "1",
    "DISABLE_TELEMETRY": "1"
  },
  "includeCoAuthoredBy": false,
  "mcpServers": {
    "bash-history": {
      "args": ["/Users/nathanvale/code/dotfiles/.claude/mcp-servers/bash-history/index.js"],
      "command": "node",
      "env": {},
      "type": "stdio"
    },
    "context7": {
      "args": ["-y", "@upstash/context7-mcp"],
      "command": "npx",
      "env": {},
      "type": "stdio"
    },
    "filesystem": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/Users/nathanvale/code",
        "/Users/nathanvale/Documents"
      ],
      "command": "npx"
    },
    "magic": {
      "args": ["-y", "@21st-dev/magic@latest", "API_KEY=\"api-key\""],
      "command": "npx",
      "env": {},
      "type": "stdio"
    },
    "mcp-server-firecrawl": {
      "command": "/bin/zsh",
      "args": ["-c", "source ~/code/dotfiles/.env && exec npx -y firecrawl-mcp"]
    },
    "playwright": {
      "args": ["@playwright/mcp@latest"],
      "command": "npx",
      "env": {},
      "type": "stdio"
    },
    "puppeteer": {
      "args": ["-y", "puppeteer-mcp-server"],
      "command": "npx",
      "env": {}
    },
    "sequential-thinking": {
      "args": ["-y", "@modelcontextprotocol/server-sequential-thinking"],
      "command": "npx",
      "env": {},
      "type": "stdio"
    },
    "tavily-mcp": {
      "command": "/bin/zsh",
      "args": ["-c", "source ~/code/dotfiles/.env && exec npx -y tavily-mcp@0.1.2"],
      "type": "stdio"
    },
    "wallaby": {
      "args": ["-y", "-c", "node ~/.wallaby/mcp/"],
      "command": "npx",
      "env": {},
      "type": "stdio"
    }
  },
